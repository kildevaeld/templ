#!/usr/bin/env node
/* jshint node:true */
'use strict';
const fs = require('fs');
const templ = require('../lib/index');

const argv = process.argv;

if (argv.length < 3) {
	console.log('usage: templ <file>')
	process.exit(1);
}


var str = fs.readFileSync(argv[2], 'utf8');
var result = templ.compile(str)

console.log(result.toString())


